{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**IdeaRS.OpenModel.csproj"
          ],
          "src": "../src"
        }
      ],
      "outputFormat": "apiPage",
      "dest": "api-iom",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "**IdeaStatiCa.BimApi.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "api-bimapi",
      "outputFormat": "apiPage",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "**IdeaStatiCa.BimApiLink.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "api-bimapilink",
      "outputFormat": "apiPage",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "**IdeaStatiCa.Plugin.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "api-plugin",
      "outputFormat": "apiPage",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "**IdeaStatiCa.RcsClient.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "api-rcs",
      "outputFormat": "apiPage",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "template": [
      "default",
      "modern",
      "templates/idea_template"
    ],
    "xref": [
      "xrefmap.yml"
    ],
    "content": [
      { "files": ["docs/**/*.{md|yml}"]},
      { "files": ["articles/*.{md|yml}"]},
      { "files": ["api-iom/**.yml", "api-iom/index.md"] },
      { "files": ["api-bimapi/**.yml", "api-bimapi/index.md" ]},
      { "files": ["api-bimapilink/**.yml", "api-bimapilink/index.md"]},
      { "files": ["api-plugin/**.yml","api-plugin/index.md"]},
      { "files": ["api-rcs/**.yml","api-rcs/index.md"]},
      { "files": ["toc.yml", "*.md"]},
      { "files": "**/*.{md|yml}" , "src": "../examples", "dest":"examples"}
    ],
    "resource": [
      { "files": ["**/images/**", "api-iom/Images/**"] },
      { "files": ["**/images/**", "**/*.cs"], "src" : "../examples", "dest":"examples"},
      { "files": "**/*.{cs|ideaCon|ideaRcs}", "src" : "../examples"}
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "IDEA StatiCa API",
      "_enableSearch": true
    },
    "dest": "_site",
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}