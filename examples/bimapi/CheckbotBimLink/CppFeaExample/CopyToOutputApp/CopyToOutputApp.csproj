	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\CppFeaApp\CppFeaApp.vcxproj" />
		<ProjectReference Include="..\ImporterWrappers\ImporterWrappers.csproj" />
	</ItemGroup>

</Project>
