<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net48</TargetFramework>
		<Configurations>Debug;Release;Debug_NuGet;Release_NuGet</Configurations>
		<UseWPF>true</UseWPF>
	</PropertyGroup>

	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
		<DebugType>full</DebugType>
		<TreatWarningsAsErrors>True</TreatWarningsAsErrors>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug_NuGet|AnyCPU'">
		<DebugType>full</DebugType>
		<TreatWarningsAsErrors>True</TreatWarningsAsErrors>
	</PropertyGroup>

	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
		<DebugType>pdbonly</DebugType>
		<TreatWarningsAsErrors>True</TreatWarningsAsErrors>
	</PropertyGroup>

	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release_NuGet|AnyCPU' ">
		<DebugType>pdbonly</DebugType>
		<TreatWarningsAsErrors>True</TreatWarningsAsErrors>
	</PropertyGroup>


	<Import Project="..\..\..\..\..\src\IdeaStatiCa.BulkSelection\IdeaStatiCa.BulkSelection.projitems" Label="Shared" />

	<ItemGroup>
		<ProjectReference Include="..\CadExampleApi\CadExampleApi.csproj" />
	</ItemGroup>

	<ItemGroup Condition="$(Configuration.Contains('NuGet'))">
		<PackageReference Include="IdeaStatiCa.PluginsTools" Version="20.1.1" />
	</ItemGroup>

	<ItemGroup Condition="'$(Configuration)' == 'Debug' Or '$(Configuration)' == 'Release'">
		<ProjectReference Include="..\..\..\..\..\src\IdeaStatiCa.PluginsTools\IdeaStatiCa.PluginsTools.csproj" />
	</ItemGroup>

</Project>
